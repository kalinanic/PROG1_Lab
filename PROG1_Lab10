#include <stdio.h>

float MetertoYard(float meter){
    float yard;
    yard = meter/0.9144;
    return yard;
}

float YardtoFoot(float yard){
    float foot;
    foot = 3.0*yard;
    return foot;
}

int main()
{
    float lenght;
    scanf("%f", &lenght);
    printf("%.2f metr贸w to %.2f jard贸w\n", lenght, MetertoYard(lenght));
    printf("%.2f jard贸w to %.2f st贸p\n", lenght, YardtoFoot(lenght));
    return 0;
}
